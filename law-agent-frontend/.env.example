# Law Agent Frontend Environment Variables

# API Endpoints
REACT_APP_API_URL=http://localhost:8000
REACT_APP_ANALYTICS_URL=http://localhost:8002
REACT_APP_DOCUMENT_URL=http://localhost:8001

# Supabase Configuration
REACT_APP_SUPABASE_URL=https://your-project.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your-supabase-anon-key

# Environment
REACT_APP_ENVIRONMENT=development

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_DOCUMENT_UPLOAD=true
REACT_APP_ENABLE_3D_COURT=true
REACT_APP_ENABLE_VOICE_CHAT=false

# Analytics Configuration
REACT_APP_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
REACT_APP_HOTJAR_ID=your-hotjar-id

# Legal Configuration
REACT_APP_DEFAULT_JURISDICTION=US
REACT_APP_SUPPORTED_LANGUAGES=en,es,fr

# Performance Configuration
REACT_APP_API_TIMEOUT=30000
REACT_APP_MAX_FILE_SIZE=52428800
REACT_APP_CHUNK_SIZE=1048576

# Security Configuration
REACT_APP_ENABLE_CSP=true
REACT_APP_ENABLE_HTTPS_ONLY=true

# Development Configuration
REACT_APP_DEBUG_MODE=false
REACT_APP_MOCK_API=false
REACT_APP_LOG_LEVEL=info
